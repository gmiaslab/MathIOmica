Cell["\<\
Here we create a function to generate some randomly sampled linear time \
series:\
\>", "ExampleText",
 CellID->949807603],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"timeSeriesLine", "[", "n_", "]"}], ":=", 
   RowBox[{"SortBy", "[", 
    RowBox[{
     RowBox[{"RandomSample", "[", 
      RowBox[{
       RowBox[{"Transpose", "[", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"Range", "[", 
           RowBox[{"1", ",", "100", ",", "1"}], "]"}], ",", 
          RowBox[{"Range", "[", 
           RowBox[{"1", ",", "100", ",", "1"}], "]"}]}], "}"}], "]"}], ",", 
       "n"}], "]"}], ",", "First"}], "]"}]}], ";"}]], "Input",
 CellLabel->"In[4]:=",
 CellID->901068345],

Cell["And also a function to generate the periodograms pairs:", "ExampleText",
 CellID->1702219222],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"lineUnevenLS", "[", "n_", "]"}], ":=", " ", 
   RowBox[{"LombScargle", "[", 
    RowBox[{
     RowBox[{"timeSeriesLine", "[", "n", "]"}], " ", ",", " ", 
     RowBox[{"Range", "[", 
      RowBox[{"1", ",", "100", ",", "1"}], "]"}], ",", 
     RowBox[{"PairReturn", "\[Rule]", " ", "True"}]}], "]"}]}], " ", 
  ";"}]], "Input",
 CellLabel->"In[5]:=",
 CellID->200995771],

