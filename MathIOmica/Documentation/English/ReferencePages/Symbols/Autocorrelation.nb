 RowBox[{
  RowBox[{
   RowBox[{"sineSignalAuto", "[", "n_", "]"}], " ", ":=", " ", 
   RowBox[{"Autocorrelation", "[", 
    RowBox[{
     RowBox[{"Transpose", "[", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"Range", "[", 
         RowBox[{"1", ",", "100"}], "]"}], ",", " ", 
        RowBox[{"Sin", "[", 
