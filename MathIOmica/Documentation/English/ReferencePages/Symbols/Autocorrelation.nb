   TraditionalForm]], "Notes"], "Notes"],
 StyleBox[Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{"\[LeftAngleBracket]", 
     RowBox[{
      RowBox[{"x", "(", "t", ")"}], " ", 
      RowBox[{"x", "(", 
       RowBox[{"t", " ", "+", " ", "\[Tau]"}], ")"}]}], 
     "\[RightAngleBracket]"}], 
    RowBox[{" ", 
