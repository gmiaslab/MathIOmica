Cell[TextData[{
 StyleBox["By the Wiener-Khinchin Theorem, if a signal ", "Notes"],
 StyleBox["x", "Notes",
  FontSlant->"Italic"],
 StyleBox["(", "Notes"],
 StyleBox["t", "Notes",
