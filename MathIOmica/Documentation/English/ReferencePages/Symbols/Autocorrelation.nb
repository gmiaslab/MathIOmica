 RowBox[{
  RowBox[{
   RowBox[{"timeSeriesLine", "[", "n_", "]"}], ":=", 
   RowBox[{"SortBy", "[", 
    RowBox[{
     RowBox[{"RandomSample", "[", 
      RowBox[{
       RowBox[{"Transpose", "[", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"Range", "[", 
           RowBox[{"1", ",", "100", ",", "1"}], "]"}], ",", 
          RowBox[{"Range", "[", 
           RowBox[{"1", ",", "100", ",", "1"}], "]"}]}], "}"}], "]"}], ",", 
       "n"}], "]"}], ",", "First"}], "]"}]}], ";"}]], "Input",
 CellLabel->"In[6]:=",
 CellID->1334376083],

